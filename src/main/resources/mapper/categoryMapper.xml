<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.api.keyword.category.CategoryMapper">
    <select id="getCategoryByWholeName" resultType="com.example.demo.api.keyword.category.Category">
        SELECT *
        FROM category
        WHERE whole_name = #{wholeName}
    </select>
</mapper>